{
    "version": "1.0.0",
    "manifest_version": 3,
    "name": "Quick Notes",
    "description": "This is a Chrome extension built with React and TypeScript that allows users to take quick notes.",
    "action": {
        "default_popup": "js/index.html",
        "default_title": "Quick Notes"
    },
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "image16.png",
        "32": "image32.png",
        "48": "image48.png",
        "128": "image128.png"
    }
}